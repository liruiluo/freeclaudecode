# AnyRouter Claude AI Configuration
# Copy this file to .env and fill in your actual values

# Your AnyRouter API Key
# Get your free $50 credits at: https://anyrouter.top/register?aff=UBdY
ANYROUTER_API_KEY=your-api-key-here

# API Base URL (usually no need to change)
ANYROUTER_BASE_URL=https://api.anyrouter.top

# Default Claude Model to use
CLAUDE_MODEL=claude-3-sonnet-20240229

# Default settings
DEFAULT_MAX_TOKENS=1000
DEFAULT_TEMPERATURE=0.7
DEFAULT_TOP_P=0.9

# Timeout settings (in seconds)
REQUEST_TIMEOUT=30
CONNECT_TIMEOUT=10

# Rate limiting
MAX_RETRIES=3
RETRY_DELAY=1

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Development settings
ENVIRONMENT=development
DEBUG=false 